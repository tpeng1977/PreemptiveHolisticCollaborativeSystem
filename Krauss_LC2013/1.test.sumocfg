<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="1.test.net.xml"/>
        <route-files value="1.test.rou.xml"/>

    </input> 

    <time>
        <begin value="0"/>
        <end value="3600"/>
        <step-length value="0.01"/>
    </time>

<!--    <output>-->
<!--        <emission-output value="1.fuel.xml"/>-->
<!--    </output>-->

    <report>
        <no-warnings value="false"/>
        <no-step-log value="true"/>
    </report>

    <!--自动跑仿真-->
    <gui_only>
        <gui-settings-file value="1.test.view.xml"/>
        <start value="true"/>
        <delay value="0"/>
        <!--仿真结束后自动关闭-->
        <quit-on-end value="true"/>
    </gui_only>
</configuration>